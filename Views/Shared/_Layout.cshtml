<!DOCTYPE html>
<html lang="en">
<head>   
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <title>@ViewData["Title"] - Landed MVC</title>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.7/css/jquery.dataTables.css" />
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
    @* <link rel="stylesheet" href="~/LandedMVC.styles.css" asp-append-version="true" /> *@
    <noscript><link rel="stylesheet" href="~/css/noscript.css" asp-append-version="true" /></noscript>
</head>
<body class="is-preload landing">
    <div id="page-wrapper">
        <header id="header">
            <h1 id="logo"><a class="navbar-brand" asp-action="Index" asp-controller="Home">Landed MVC</a></h1>
            <nav id="nav">
                <ul>
                    <li><a class="navbar-brand" asp-action="Index"  asp-controller="Home">Home</a></li>
                    <li>
                        <a href="#">MVC</a>
                        <ul>
                            <li><a asp-action="index" asp-controller="users">Users</a></li>
                            <li><a asp-action="index" asp-controller="users">Events</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">My Other Projects</a>
                        <ul>
                            <li><a href="https://resume.mypitech.com" target="_blank">Resume/Bio</a></li>
                            <li><a href="right-sidebar.html">Simple MVC</a></li>
                            <li><a asp-action="index" asp-controller="users">API Swagger UI</a></li>
                            <li><a href="no-sidebar.html">Simple SignalR</a></li>
                            <li>
                                <a href="#">Repositories</a>
                                <ul>
                                    <li><a href="#">This Repo</a></li>
                                    <li><a href="#">Simple MVC Repo</a></li>
                                    <li><a href="#">Simple SignalR Repo</a></li>
                                    <li><a href="#">API Repo</a></li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li><a href="/#five" class="button primary">Contact Me</a></li>
                </ul>
            </nav>
        </header>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <footer id="footer">
            <ul class="icons">
                <li><a href="#" class="icon solid alt fa-envelope"><span class="label">Email</span></a></li>
                <li><a href="#" class="icon brands alt fa-linkedin-in"><span class="label">LinkedIn</span></a></li>
                <li><a href="#" class="icon brands alt fa-github"><span class="label">GitHub</span></a></li>
            </ul>
            <ul class="copyright">
                <li>&copy; 2024 - Landed MVC. All rights reserved.</li>
                <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
                <li>Demo Images: <a href="http://unsplash.com">Unsplash</a></li>
                <li>About Me: <a href="https://resume.mypitech.com">Shawn Wheeler</a></li>
            </ul>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/jquery.scrolly.min.js"></script>
    <script src="~/js/jquery.dropotron.min.js"></script>
    <script src="~/js/jquery.scrollex.min.js"></script>
    <script src="~/js/browser.min.js"></script>
    <script src="~/js/breakpoints.min.js"></script>
    <script src="~/js/util.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
